# Ricochet Environment Variables
# Copy this file to .env and fill in your tokens

# Required: Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=

# Optional: Discord Bot Token (from Discord Developer Portal)
DISCORD_BOT_TOKEN=

# Optional: Restrict Discord bot to specific guild (server)
DISCORD_GUILD_ID=

# Optional: Restrict Telegram bot to specific user IDs (comma-separated)
ALLOWED_USER_IDS=
